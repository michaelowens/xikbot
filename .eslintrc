{
    "plugins": ["html"],
    "rules": {
        "arrow-spacing": [2],
        "camelcase": [2],
        "eqeqeq": [2],
        "indent": [2],
        "padded-blocks": [2, "never"],
        "prefer-spread": [1],
        "prefer-template": [1],
        "prefer-arrow-callback": [2],
        "no-eval": [2],
        "no-fallthrough": [2],
        "no-implied-eval": [2],
        "no-labels": [2],
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-multi-spaces": [1],
        "no-var": [1],
        "object-curly-spacing": [2, "never"],
        "quotes": [2, "single"],
        "radix": [2],
        "semi": [2, "never"],
        "vars-on-top": [1],
        "yoda": [2]
    },
    "parser": "babel-eslint"
}